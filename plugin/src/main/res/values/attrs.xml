<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="isSelect" format="boolean" />
    <declare-styleable name="ImageViewCanSelect">
        <attr name="isSelect" />
        <attr name="image_type" format="enum">
            <enum name="icon" value="0" />
            <enum name="photo" value="1" />
        </attr>
        <attr name="border_shape" format="enum">
            <enum name="round_rect" value="0" />
            <enum name="circle" value="1" />
        </attr>
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="TextViewCanSelect">
        <attr name="isSelect" />
    </declare-styleable>

    <declare-styleable name="ProgressBar">
        <attr name="activeLineColor" format="color"/>
        <attr name="inactiveLineColor" format="color"/>
        <attr name="circleColor" format="color"/>
        <attr name="textColor" format="color"/>
        <attr name="lineHeight" format="dimension"/>
        <attr name="circleRadius" format="dimension"/>
        <attr name="delayShowText" format="integer"/>
        <attr name="textPadding" format="dimension"/>
        <attr name="animationTime" format="integer"/>
        <attr name="textSize" format="dimension"/>
        <attr name="textHeight" format="dimension"/>
        <attr name="progress" format="float"/>
        <attr name="closePadding" format="boolean"/>
        <attr name="closeText" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="bubble">
        <attr name="bubble_shadowColor" format="color" />
        <attr name="bubble_padding" format="dimension" />
        <attr name="bubble_strokeWidth" format="float" />
        <attr name="bubble_cornerRadius" format="float" />
        <attr name="bubble_halfBaseOfLeg" format="dimension" />
    </declare-styleable>
</resources>